{
    "env": {
        "commonjs": true,
        "es2021": true,
        "amd": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "ignorePatterns": [
        "node_modules/**/*.js"
    ],
    "overrides": [
        {
        "files": [
            "**/*.test.js"
        ],
        "env": {
            "jest": true
        }
        }
    ]
}
  